@{
	ViewData["Title"] = "Departament List";

}


<meta name="viewport" content="width-device-width, initial-scale=1.0" />
<link rel="stylesheet" href="~/css/GerenciarUsuarios.css">

<div class="container-conteudo">

	<h2 class="work-sans">Departament List</h2>

	<table class="table">
		<thead>
			<tr>
				<th scope="col">#</th>
				<th scope="col">Department Name</th>
				<th scope="col">Manager email</th>
				<th scope="col">Edit </th>
				<th scope="col">Active</th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<th scope="row">1</th>
				<td>Marketing</td>
				<td>annie@gmail.com</td>
				<td>
					<a> <img role="button" class="mini-icons" src="~/images/pencil.svg" /> </a>
				</td>
				<td>
					<div class="form-check form-switch">
						<input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheckChecked" checked>
						<label class="form-check-label" for="flexSwitchCheckChecked"></label>
					</div>
				</td>
			</tr>
			<tr>
				<th scope="row">2</th>
				<td>Negócios</td>
				<td>Bianca@gmail.com</td>
				<td>
					<a> <img role="button" class="mini-icons" src="~/images/pencil.svg" /> </a>
				</td>
				<td>
					<div class="form-check form-switch">
						<input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheckChecked" checked>
						<label class="form-check-label" for="flexSwitchCheckChecked"></label>
					</div>
				</td>
			</tr>
			<tr>
				<th scope="row">3</th>
				<td>Tech</td>
				<td>Fulano@gmail.com</td>
				<td>
					<img role="button" class="mini-icons" src="~/images/pencil.svg" />
				</td>
				<td>

					<div class="form-check form-switch">
						<input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheckChecked" checked>
						<label class="form-check-label" for="flexSwitchCheckChecked"></label>
					</div>
				</td>
			</tr>
			</div>
		</tbody>
	</table>

</div>
<div class="btn-position">
	<button>New Departament</button>
</div>

